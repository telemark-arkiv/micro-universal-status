{
  "name": "micro-universal-status",
  "description": "Shows status for service",
  "version": "1.0.3",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=7.5.0"
  },
  "scripts": {
    "test": "standard && nsp check && tap --reporter=spec test/**/*.js",
    "test-offline": "standard && tap --reporter=spec test/**/*.js",
    "coverage": "tap test/**/*.js --coverage",
    "coveralls": "tap --cov --coverage-report=lcov test/**/*.js && cat coverage/lcov.info | coveralls",
    "setup": "npm install",
    "standard-fix": "standard --fix",
    "start": "micro -p 8080",
    "dev": "micro",
    "now-deploy": "npm test && now --npm --alias"
  },
  "keywords": [
    "microservice",
    "micro",
    "status",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/micro-universal-status.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/micro-universal-status/issues"
  },
  "now": {
    "aliases": [
      "universal.status.micro.t-fk.no"
    ],
    "env": {
      "NODE_ENV": "production"
    }
  },
  "devDependencies": {
    "coveralls": "2.11.16",
    "nsp": "2.6.3",
    "standard": "8.6.0",
    "tap": "10.2.0"
  },
  "dependencies": {
    "got": "6.7.1",
    "marked": "0.3.6",
    "micro": "7.0.2"
  }
}
